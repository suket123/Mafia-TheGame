<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.github.suket123.mafia_thegame">

    <application
        android:allowBackup="true"
        android:icon="@drawable/appimage"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Characters" />
        <activity android:name=".TapToSeeCard" />
        <activity android:name=".Mafia" />
        <activity android:name=".Police" />
        <activity android:name=".Doctor" />
        <activity android:name=".Citizen" />
        <activity android:name=".Instructions"></activity>
    </application>

</manifest>